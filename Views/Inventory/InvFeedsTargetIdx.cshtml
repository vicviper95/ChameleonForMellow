@{
  ViewData["Title"] = "Inventory Feeds";
  Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>&nbsp;&nbsp;@ViewData["Title"] - Target</h1>
<input type="hidden" id="historyDateHidden" name="historyDateHidden" />

<br />
<div class="invFeed-container">
  <div class="col-6">
    <div>
      <h2 class="text-info">Target Inventory Feeds</h2>
      <h7><p id="invFeedsRepDate"></p></h7>
    </div>

  </div>
  <div class="col-12 border p-3">
    

    <table style="width:100%">
      <tbody>
        <tr>
          <td>
            <input type="button" id="bttnForSendingToEDI" class="btn btn-primary" style="float: right" value="Send" />
          </td>
          <td></td>
          <td></td>
          <td>
            <div style=" padding-top: 10px">
              Past Feeds: <input type="text" id="historyDatePicker">
              <button id="reloadTargetFeedsHistory"> reload</button>
            </div>
            
        </tr>
        <tr>
          <td colspan="2" style="width:35%">
            <table id="invFeedsTargetPastFeedsItems" class="table tabel-striped table-bordered display compact" style="width:100%; font-size: 10px;border: 1px solid CornflowerBlue;border-radius: 10px; border-collapse: separate;">
              <thead>
                <tr style="font-size: 10px;">
                  <th>Warehouse</th>
                  <th>Feed Date</th>
                  <th>Time Sent</th>
                  <th>Sent By</th>
                </tr>
              </thead>
            </table>
          </td>
          <td colspan="2" style="width:65%">
            Today's Feeds
            <table id="invFeedsTargetItems" class="table tabel-striped table-bordered display compact" style="width:100%; font-size: 10px;border: 1px solid CornflowerBlue;border-radius: 10px; border-collapse: separate;">
              <thead>
                <tr style="font-size: 10px;">
                  <th>SKU</th>
                  <th>Customer SKU</th>
                  <th>Customer UPC</th>
                  <th>Main SL</th>
                 @*  <th>SW-CAFT</th> *@
                  @* <th>BANC</th>
                  <th>BASC</th> *@
                  @* <th>PRISM-CAST</th> *@
                  <th>ZINUS-Tracy</th>
                  <th>ZINUS-CHS</th>
                </tr>
              </thead>
            </table>
          </td>
        </tr>
      </tbody>
    </table>

  </div>
</div>

<div class="modal"><!-- For waiting indicator--></div>

@section Scripts{
<script>
    function lastmod() { }
</script>
<script src="~/js/InvFeedsTargetList.js">
</script>
}